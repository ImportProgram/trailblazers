{
    "presets": [
        "@babel/env",
        "@babel/react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-spread",
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es"
            }
        ]
    ]
}